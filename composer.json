{
    "name": "uppes/coroutine",
    "description": "Coroutine task library to implement cooperative multitasking by yields & generators, and offering with parallel processing",
	"keywords": [
		"cooperative",
		"multitasking",
		"coroutine",
		"task",
		"yield",
        "parallel",
        "process",
		"generator",
        "async"
	],
    "homepage": "https://github.com/uppes/coroutine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Stubbs",
            "email": "lstubbs@techno.express"
        },
        {
            "name": "Nikita Popov",
            "email": "nikic@php.net"
        }
    ],
    "require": {
        "php": ">7.0",
        "uppes/processor": "^1.0.7"
    },
    "autoload": {
        "files": [
            "Coroutine/Core.php"
        ],
        "psr-4": { 
			"Async\\Coroutine\\": "Coroutine/"
		}
    },
    "require-dev": {
        "phpunit/phpunit": ">5.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Async\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    }
}
