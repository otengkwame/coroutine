{
    "name": "uppes/coroutine",
    "description": "A Coroutine task library to implement cooperative multitasking by yields & generators.",
	"keywords": [
		"cooperative",
		"multitasking",
		"coroutine",
		"task",
		"yield",
		"Generator"
	],
    "homepage": "https://github.com/uppes/coroutine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Stubbs",
            "email": "lstubbs@techno.express"
        },
        {
            "name": "Nikita Popov",
            "email": "nikic@php.net"
        }
    ],
    "require": {
        "php": ">7.0"
    },
    "autoload": {
        "files": [
            "Coroutine/global.php"
        ],
        "psr-4": { 
			"Async\\Coroutine\\": "Coroutine/"
		}
    },
    "require-dev": {
        "phpunit/phpunit": ">5.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Async\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    }
}
